<form action='/user/logging_in' method='POST'>
	<div style="text-align: center; width: 500px ">
		<div class="form-group">
			<label>User Name</label>
			<input type="username" class="form-control" id="username" name="username" placeholder="Enter User Name">
		</div>
		<div class="form-group">
			<label>Password</label>
			<input type="password" class="form-control" id="password" name="password" placeholder="Enter User Password">
		</div>
		<button type="submit" class="btn btn-primary">Submit</button>
	</div>
</form>


